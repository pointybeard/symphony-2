{
    "name": "pointybeard/helpers-exceptions-readabletrace",
    "version": "1.0.2",
    "description": "Provides an exception base class that includes method getReadableTrace(), giving simple access to a plain-text, readable, backtrace string.",
    "homepage": "https://github.com/pointybeard/helpers-exceptions-readabletrace",
    "license": "MIT",
    "authors": [
        {
            "name": "Alannah Kearney",
            "email": "hi@alannahkearney.com",
            "homepage": "http://alannahkearney.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "pointybeard/helpers-functions-paths": "~1",
        "pointybeard/helpers-functions-strings": "~1",
        "pointybeard/helpers-functions-debug": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "support": {
        "issues": "https://github.com/pointybeard/helpers-exceptions-readabletrace/issues",
        "wiki": "https://github.com/pointybeard/helpers-exceptions-readabletrace/wiki"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "pointybeard\\Helpers\\Exceptions\\": "src/"
        }
    }
}
