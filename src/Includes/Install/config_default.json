{
    "symphony": {
        "admin-path": "symphony",
        "pagination_maximum_rows": 50,
        "association_maximum_rows": 5,
        "lang": "en",
        "pages_table_nest_children": "no",
        "version": null,
        "cookie_prefix": "sym-",
        "session_gc_divisor": 10,
        "cell_truncation_length": 75,
        "enable_xsrf": "yes",
        "error_reporting_all": "no"
    },
    "log": {
        "archive": 1,
        "maxsize": 102400,
        "filter": null
    },
    "database": {
        "host": "localhost",
        "port": "3306",
        "user": null,
        "password": null,
        "db": null,
        "tbl_prefix": "sym_",
        "query_caching": "on",
        "query_logging": "on"
    },
    "public": {
        "display_event_xml_in_source": "no"
    },
    "general": {
        "sitename": "Symphony CMS",
        "useragent": null
    },
    "file": {
        "write_mode": "0644"
    },
    "directory": {
        "write_mode": "0755"
    },
    "region": {
        "time_format": "g:i a",
        "date_format": "m/d/Y",
        "datetime_separator": " ",
        "timezone": null
    },
    "cache_driver": {
        "default": "database"
    },
    "admin": {
        "max_upload_size": 5242880,
        "upload_blacklist": "/\\\\.(?:php[34567s]?|phtml)$/i"
    },
    "sorting": {
    }
}
